<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.augustana.csc490.animalfinder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EasyPanda"
            android:label="@string/title_activity_easy_panda" >
        </activity>
        <activity
            android:name=".MediumPanda"
            android:label="@string/title_activity_medium_panda" >
        </activity>
        <activity
            android:name=".HardPanda"
            android:label="@string/title_activity_hard_panda" >
        </activity>
    </application>

</manifest>
